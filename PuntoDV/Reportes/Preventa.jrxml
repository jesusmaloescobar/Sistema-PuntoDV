   n.fecha,n.id_ruta,r.descripcion,e.nombre,sum(cant_tot) as cantidad,sum(subtotal) subtotal,sum(descuento) descuento,sum(sub_total) as sub_total,sum(iva) as iva,sum(total) as total
FROM
   notas n, ruta r, empleado e
